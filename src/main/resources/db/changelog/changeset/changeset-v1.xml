<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
      http://www.liquibase.org/xml/ns/pro
      http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <changeSet id="1" author="karthik">
        <createTable tableName="CUSTOMER_INFO_BATCH">
            <column name="id" type="int">
                <constraints primaryKey="true"/>
            </column>
            <column name="first-name" type="varchar(255)"></column>
            <column name="last-name" type="varchar(255)"></column>
            <column name="email" type="varchar(255)"></column>
            <column name="gender" type="varchar(255)"></column>
            <column name="contact-number" type="varchar(255)"></column>
            <column name="country" type="varchar(255)"></column>
            <column name="dob" type="varchar(255)"></column>
        </createTable>
    </changeSet>



</databaseChangeLog>

